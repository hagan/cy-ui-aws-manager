{
  "name": "awsmgr",
  "version": "0.0.1",
  "description": "AWS Manager UX",
  "main": "server/index.js",
  "private": true,
  "_moduleAliases": {
    "@": "src"
  },
  "scripts": {
    "test": "jest",
    "format": "pretty-quick --no-restage",
    "format:staged": "pretty-quick --staged",
    "format:all": "prettier --write .",
    "format-check": "pretty-quick --check",
    "prepare": "# [ ${SKIP_PREPARE:-0} -eq 1 ] && echo 'Skipping husky install' || husky install",
    "dev": "next dev",
    "build": "yarn lint && yarn next build",
    "pack": "find ./dist/ -mindepth 1 ! -name '.gitkeep' -exec rm -rf {} + && yarn pack --out ./dist/%s-v%v.tgz",
    "start": "next start",
    "lint": "echo 'running lint' && eslint . --ext .ts,.tsx",
    "start-express-server-trace": "node --trace-deprecation ./server/index.js",
    "start-express-server": "node ./server/index.js"
  },
  "files": [
    ".next",
    ".nvmrc",
    "awsmgr-start.mjs",
    "bin",
    "declarations.d.ts",
    "next-env.d.ts",
    "next.config.js",
    "public",
    "package.json",
    "yarn.lock",
    "package-lock.json",
    "postcss.config.js",
    "server",
    "src",
    "tsconfig.json"
  ],
  "bin": {
    "awsmgr-start": "./awsmgr-start.mjs"
  },
  "repository": "git+https://github.com/hagan/cy-ui-aws-manager.git",
  "author": "Hagan Franks / University of Arizona / Data Science Institute",
